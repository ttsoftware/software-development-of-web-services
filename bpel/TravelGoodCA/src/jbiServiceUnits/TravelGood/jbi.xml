    <services binding-component="false">
        <provides endpoint-name="TravelAgencySoapInterfaceRole_myRole" interface-name="ns1:TravelAgencySoapInterface" service-name="ns0:TravelGoodClient">
            <ns2:display-name>TravelGoodClient</ns2:display-name>
            <ns2:process-name>TravelGood</ns2:process-name>
